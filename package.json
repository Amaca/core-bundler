{
  "author": "Massimo Carletti <carletti.massimo@gmail.com>",
  "name": "core-bundler",
  "description": "Static site boilerplate based on webpack to lazy load .js, .html and assets on scroll to increase performances",
  "keywords": [
    "bundler",
    "es6",
    "webpack",
    "lazyload",
    "corevitals"
  ],
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/amaca/core-bundler.git"
  },
  "scripts": {
    "build": "webpack --config ./bundler/webpack.prod.js",
    "dev": "webpack serve --config ./bundler/webpack.dev.js",
    "deploy": "vercel --prod"
  },
  "dependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "babel-loader": "^8.2.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.5.0",
    "file-loader": "^6.2.0",
    "html-loader": "^3.0.0",
    "html-webpack-plugin": "^5.5.0",
    "internal-ip": "^6.2.0",
    "mini-css-extract-plugin": "^2.4.3",
    "portfinder-sync": "0.0.2",
    "raw-loader": "^4.0.2",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "swiper": "^8.0.7",
    "vercel": "^28.15.3",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0",
    "webpack-merge": "^5.8.0"
  },
  "devDependencies": {
    "nunjucks": "^3.2.3",
    "simple-nunjucks-loader": "^3.2.0",
    "svg-sprite-html-webpack": "^2.4.0"
  }
}
